{
  "name": "mesilov/bitrix24-php-sdk",
  "description": "A powerful PHP library for the Bitrix24 REST API",
  "keywords": [
    "Bitrix24",
    "PHP",
    "REST",
    "API"
  ],
  "type": "library",
  "homepage": "https://github.com/mesilov/bitrix24-php-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Maxim Mesilov",
      "homepage": "https://github.com/mesilov/"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ext-curl": "*",
    "psr/log": "1.1.3",
    "symfony/http-client": "5.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.3.*",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Bitrix24\\SDK\\": "src"
    }
  },
  "scripts": {
    "test": [
      "phpunit --colors=always --verbose"
    ]
  }
}